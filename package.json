{
  "name": "miles_creative_bot",
  "module": "index.ts",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "bun --watch index.ts",
    "start": "bun run build/index.js",
    "build": "bun build --target=bun index.ts  --outdir ./build",
    "typecheck": "bun type-check",
    "lint": "bun lint",
    "docker:build": "docker build --pull -t miles_creative_bot .",
    "docker:run": "docker run -p 3000:3000 miles_creative_bot"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
